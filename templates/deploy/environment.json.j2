{
  "SECRET_KEY": "{{ secret_key }}",
  "ALLOWED_HOSTS": ["{{ app_domain }}"],
  "CELERY_BROKER_URL": "amqp://{{ app_user }}:{{ broker_password }}@localhost:5672//",
  "DATABASES": {
    "default": {
      "ENGINE": "django.db.backends.postgresql",
      "NAME": "{{ db_name }}",
      "USER": "{{ db_username }}",
      "PASSWORD": "{{ db_password }}",
      "HOST": "/var/run/postgresql",
      "PORT": "{{ db_pool_port }}"
    }
  }
}